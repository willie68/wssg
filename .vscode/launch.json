{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [

    {
      "name": "wssg init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "init",
        "-f",
        "./example"
      ]
    },
    {
      "name": "wssg new page",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "page",
        "contact/index",
        "-f",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg new page plugin",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "page",
        "gallery2/index",
        "-f",
        "-p",
        "gallery",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg new section",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "section",
        "gallerie",
        "-f",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "version",
      ]
    },
    {
      "name": "wssg generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "generate",
        "-f",
        "--verbose",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg serve",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "serve",
        "--verbose",
        "-r",
        "./example"
      ]
    }
  ]
}