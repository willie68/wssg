  "version": "0.2.0",
  "configurations": [

    {
      "name": "wssg init",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "init",
        "-f",
        "./example"
      ]
    },
    {
      "name": "wssg new page",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "page",
        "contact/index",
        "-f",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg new page plugin",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "page",
        "blog/blog1",
        "-p", "blog",
        "-f",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg new section plugin",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "new",
        "section",
        "blog",
        "-p", "blog",
        "-f",
        "-r",
        "./example"
      ]
    },
    {
      "name": "wssg version",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "version",
      ]
    },
    {
      "name": "wssg generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "generate",
        "-f",
        "-c",
        "--verbose",
        "-r",
        "./example"
      ]
    },
    {
      "name": "vs wssg generate",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "generate",
        "-f",
        "--verbose",
        "-r",
        "H:/privat/nxt-vs/Volker Sauber/sauber"
      ]
    },
    {
      "name": "wssg serve",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "serve",
        "--verbose",
        "-r",
        "./example"
      ]
    },
    {
      "name": "vs wssg serve",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/main.go",
      "cwd": "${workspaceFolder}",
      "output": "${workspaceFolder}/debug.exe",
      "args": [
        "serve",
        "--verbose",
        "-r",
        "H:/privat/nxt-vs/Volker Sauber/sauber"
      ]
    }
  ]
}